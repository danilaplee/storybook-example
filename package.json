{
  "name": "storybook-example",
  "version": "1.0.0",
  "main": "dist/lib.js",
  "scripts": {
    "build-lib": "./node_modules/.bin/webpack",
    "dev": "nodemon -e js,jsx,scss --watch src --exec \"yarn storybook\"",
    "storybook": "yarn build-lib && yarn link && yarn link 'storybook-example' && start-storybook --ci -p 6006",
    "test": "./node_modules/backstopjs/cli/index.js test",
    "backstop": "./node_modules/backstopjs/cli/index.js",
    "build-storybook": "yarn build-lib && yarn link && yarn link 'storybook-example' && build-storybook"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint",
      "git add"
    ]
  },
  "dependencies": {
